{
  "name": "vscode-extension",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/vscode-extension/src",
  "projectType": "application",
  "targets": {
    "build-backend": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "main": "apps/vscode-extension/extension/main.ts",
        "tsConfig": "apps/vscode-extension/tsconfig.extension.json",
        "outputPath": "dist/apps/vscode-extension",
        "project": "apps/vscode-extension/package.json",
        "defaultConfiguration": "production",
        "external": ["vscode"],
        "bundle": true,
        "thirdParty": true,
        "minify": false,
        "sourcemap": true,
        "format": ["cjs"],
        "additionalEntryPoints": [
          "apps/vscode-extension/extension/language-server/main.ts"
        ],
        "generatePackageJson": false,
        "deleteOutputPath": false,
        "esbuildConfig": "apps/vscode-extension/esbuild.extension.config.js",
        "assets": [
          {
            "glob": "package.json",
            "input": "apps/vscode-extension",
            "output": "/"
          },
          {
            "glob": "CHANGELOG.md",
            "input": "",
            "output": "/"
          },
          {
            "glob": "LICENSE",
            "input": "",
            "output": "/"
          },
          {
            "glob": "README.md",
            "input": "apps/vscode-extension",
            "output": "/"
          },
          {
            "input": "assets/icons",
            "glob": "{light,dark}/**/icon.png",
            "output": "assets"
          },
          {
            "input": "assets/icons",
            "glob": "{light,dark,vscode}/**/*.svg",
            "output": "assets"
          },
          {
            "input": "packages/language/syntaxes",
            "glob": "*.*",
            "output": "syntaxes"
          },
          {
            "input": "packages/language",
            "glob": "language-configuration.json",
            "output": "."
          },
          {
            "input": "packages/language/examples",
            "glob": "*.*",
            "output": "examples"
          }
        ]
      },
      "configurations": {
        "ci": {
          "optimization": false
        },
        "development": {
          "optimization": false,
          "minify": false,
          "sourcemap": true
        },
        "production": {
          "optimization": true,
          "skipTypeCheck": true,
          "minify": false
        }
      }
    },
    "build-frontend": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "main": "apps/vscode-extension/client/main.tsx",
        "tsConfig": "apps/vscode-extension/tsconfig.client.json",
        "outputPath": "dist/apps/vscode-extension/client",
        "project": "apps/vscode-extension/package.json",
        "defaultConfiguration": "production",
        "external": ["vscode"],
        "bundle": true,
        "thirdParty": true,
        "minify": false,
        "sourcemap": true,
        "deleteOutputPath": false,
        "esbuildConfig": "apps/vscode-extension/esbuild.client.config.js",
        "format": ["cjs"]
      },
      "configurations": {
        "ci": {
          "optimization": false
        },
        "development": {
          "optimization": false,
          "minify": false,
          "sourcemap": true
        },
        "production": {
          "skipTypeCheck": true,
          "minify": false
        }
      }
    },
    "build-all": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "outputPath": "dist/apps/vscode-extension",
      "defaultConfiguration": "production",
      "options": {
        "commands": [
          "pnpm nx run vscode-extension:build-backend",
          "pnpm nx run vscode-extension:build-frontend"
        ],
        "parallel": true
      },
      "configurations": {
        "ci": {
          "options": {
            "commands": [
              "pnpm nx run vscode-extension:build-backend:ci",
              "pnpm nx run vscode-extension:build-frontend:ci"
            ],
            "parallel": true
          }
        },
        "development": {
          "options": {
            "commands": [
              "pnpm nx run vscode-extension:build-backend:development",
              "pnpm nx run vscode-extension:build-frontend:development"
            ],
            "parallel": true,
            "sourcemap": true
          }
        },
        "production": {
          "options": {
            "commands": [
              "pnpm nx run vscode-extension:build-backend:production",
              "pnpm nx run vscode-extension:build-frontend:production"
            ],
            "parallel": true
          }
        }
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "outputPath": "dist/apps/vscode-extension",
      "options": {
        "commands": ["rimraf dist/packages/vscode-extension"]
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputPath}"],
      "outputPath": "dist/apps/vscode-extension",
      "defaultConfiguration": "production",
      "options": {
        "commands": [
          "pnpm nx run vscode-extension:clean",
          "pnpm nx run vscode-extension:build-all"
        ],
        "parallel": false
      },
      "configurations": {
        "ci": {
          "options": {
            "commands": [
              "pnpm nx run vscode-extension:clean",
              "pnpm nx run vscode-extension:build-all:ci"
            ],
            "parallel": false
          }
        },
        "development": {
          "options": {
            "commands": [
              "pnpm nx run vscode-extension:clean",
              "pnpm nx run vscode-extension:build-all:development"
            ],
            "parallel": false,
            "sourcemap": true
          }
        },
        "production": {
          "options": {
            "commands": [
              "pnpm nx run vscode-extension:clean",
              "pnpm nx run vscode-extension:build-all:production"
            ],
            "parallel": false
          }
        }
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm nx run vscode-extension:build",
          "pnpm pack \"./apps/vscode-extension\" --pack-destination \"./dist/apps/vscode-extension\""
        ],
        "parallel": false
      }
    },
    "publish-dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "pnpm nx run vscode-extension:build",
          "cd dist/apps/vscode-extension",
          "pnpm publish --registry http://localhost:4873"
        ],
        "parallel": false
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rimraf /S /Q \"package.json\" ",
          "renamer --replace --find \"package.vsce.json\" --replace \"package.json\" *",
          "npx vsce package --no-dependencies",
          "npx vsce publish --no-dependencies"
        ],
        "parallel": false,
        "cwd": "dist/apps/vscode-extension"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/vscode-extension/**/*.ts",
          "apps/vscode-extension/**/*.tsx",
          "apps/vscode-extension/package.json"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": []
}
